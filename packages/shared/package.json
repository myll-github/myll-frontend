    "@testing-library/react": "^14.0.0",
    "@types/testing-library__jest-dom": "^5.14.8",
    "@babel/preset-typescript": "^7.21.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "react": "^18.0.2",
    "tsconfig": "workspace:*",
    "jest-config": "workspace:*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "crypto": "^1.0.1"
  }
