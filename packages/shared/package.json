{
  "name": "shared",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
